#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pyclip==0.6.0','console_scripts','pyclip'
__requires__ = 'pyclip==0.6.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyclip==0.6.0', 'console_scripts', 'pyclip')()
    )
